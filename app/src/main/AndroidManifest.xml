<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/cabangin"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AsdaqKebab"
        tools:targetApi="31">
        <activity
            android:name=".AwalanActivity"
            android:exported="true"
            android:theme="@style/Theme.AsdaqKebab.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LaporanCabangActivity"
            android:exported="false"
            android:theme="@style/Theme.AsdaqKebab.NoActionBar" />
        <activity
            android:name=".ConfirmForgetActivity"
            android:exported="false"
            android:theme="@style/Theme.AsdaqKebab.NoActionBar" />
        <activity
            android:name=".OtpActivity"
            android:exported="false"
            android:theme="@style/Theme.AsdaqKebab.NoActionBar" />
        <activity
            android:name=".ForgetActivity"
            android:exported="false"
            android:theme="@style/Theme.AsdaqKebab.NoActionBar" />
        <activity
            android:name=".HomeActivity"
            android:exported="true"
            android:theme="@style/Theme.AsdaqKebab.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:theme="@style/Theme.AsdaqKebab.NoActionBar"/>

    </application>

</manifest>